<h6><fmt:message
        key="show_student_jspf.h6.student_name"/> ${teacherNames.get('lastName')} ${teacherNames.get('firstName')} ${teacherNames.get('patronymic')}</h6>

<c:if test="${teacherNames.get('phone') != null && not empty teacherNames.get('phone')}">
    <h6><fmt:message key="show_student_jspf.h6.student_phone"/> ${teacherNames.get('phone')}</h6>
</c:if>
<c:if test="${teacherNames.get('email') != null && not empty teacherNames.get('email')}">
    <h6><fmt:message key="show_student_jspf.h6.student_email"/> ${teacherNames.get('email')}</h6>
</c:if>
